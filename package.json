{
    "name": "deadly-duel",
    "type": "module",
    "description": "A high-performance, competitive pixel-art fighting game designed for the open web using Phaser 3 and React.",
    "version": "0.1.0",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/your-org/deadly-duel.git"
    },
    "author": "Deadly Duel Team",
    "license": "MIT",
    "licenseUrl": "http://www.opensource.org/licenses/mit-license.php",
    "bugs": {
        "url": "https://github.com/your-org/deadly-duel/issues"
    },
    "homepage": "https://github.com/your-org/deadly-duel#readme",
    "keywords": [
        "phaser",
        "phaser3",
        "react",
        "vite",
        "typescript",
        "fighting-game",
        "multiplayer"
    ],
    "scripts": {
        "dev": "node log.js dev & vite --config vite/config.dev.mjs",
        "build": "node log.js build & vite build --config vite/config.prod.mjs",
        "dev-nolog": "vite --config vite/config.dev.mjs",
        "build-nolog": "vite build --config vite/config.prod.mjs",
        "lint": "eslint src --ext ts,tsx --report-unused-disable-directives",
        "type-check": "tsc --noEmit"
    },
    "dependencies": {
        "@solana/spl-token": "^0.4.13",
        "@solana/web3.js": "^1.98.2",
        "@supabase/supabase-js": "^2.50.2",
        "@types/node": "^24.0.10",
        "autoprefixer": "^10.4.21",
        "phaser": "^3.90.0",
        "postcss": "^8.5.6",
        "react": "^19.1.0",
        "react-dom": "^19.1.0",
        "tailwindcss": "^4.1.11",
        "zustand": "^5.0.6"
    },
    "devDependencies": {
        "@eslint/js": "^9.30.1",
        "@tailwindcss/postcss": "^4.1.11",
        "@types/react": "^19.1.8",
        "@types/react-dom": "^19.1.6",
        "@vitejs/plugin-react": "^4.6.0",
        "eslint": "^9.30.1",
        "eslint-plugin-react-hooks": "^5.2.0",
        "eslint-plugin-react-refresh": "^0.4.20",
        "globals": "^16.3.0",
        "terser": "^5.43.1",
        "typescript": "~5.8.3",
        "typescript-eslint": "^8.35.1",
        "vite": "^7.0.0"
    }
}
